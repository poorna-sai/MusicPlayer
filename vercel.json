{
  "version": 2,
  "builds": [
    {
      "src": "MusicProject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "3000mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "MusicProject/wsgi.py"
    }
  ],
  "includeFiles": [
    "db.sqlite3",
    "staticfiles/**",
    "media/**",
    "MusicProject/**"
  ]
}
